<table class="calendar">
    <tr><td colspan="{{ calendar.widthRenderedCalendarInDays }}" id="calendar_month">{{ calendar.renderMonthName }}</td></tr>
    <tr>
        {%  for day in days %}
            <th>{{ day }}</th>
        {%  endfor %}
        {{ calendar.renderWeekDays }}
    </tr>
    <tr>
        {%  for count in 1..calendar.calculateOffsetAtStart %}
            {% include('emptyday.twig') %}
        {% endfor %}
        {{ calendar.renderDates }}
    </tr>
</table>